{
	"name": "PADB",
	"version": "16.1.0",
	"scripts": {
		"start-local": "ng serve --configuration=local",
		"build-dev": "ng build --configuration=dev",
		"build-dev-ci": "ng build --configuration=dev --progress=false",
		"build-prod": "ng build --configuration=prod",
		"build-prod-ci": "ng build --configuration=prod --progress=false",
		"test": "ng test",
		"test-coverage": "ng test --configuration=coverage",
		"lint-ci": "ng lint && npm run stylelint-ci",
		"lint-local": "ng lint --fix && npm run stylelint-fix",
		"stylelint-ci": "npx stylelint \"**.scss\"",
		"stylelint-fix": "npx stylelint \"**.scss\" --fix",
		"run-all-checks-local": "npm run lint-local && npm run test-coverage && npm run build-prod && npm run e2e-local",
		"compodoc": "compodoc -p tsconfig.doc.json",
		"compodoc-ci": "compodoc -p tsconfig.doc.json --coverageMinimumPerFile 80 && compodoc -p tsconfig.doc.json --unitTestCoverage coverage/PADB/coverage-summary.json",
		"sonarqube": "sonar-scanner",
		"e2e-local": "playwright test --config=playwright-local.config.ts",
		"e2e-ci": "playwright test --config=playwright-ci.config.ts",
		"postinstall": "npx playwright install chromium"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^16.2.0",
		"@angular/cdk": "^16.2.0",
		"@angular/common": "^16.2.0",
		"@angular/compiler": "^16.2.0",
		"@angular/core": "^16.2.0",
		"@angular/forms": "^16.2.0",
		"@angular/localize": "^16.2.0",
		"@angular/platform-browser": "^16.2.0",
		"@angular/platform-browser-dynamic": "^16.2.0",
		"@angular/router": "^16.2.0",
		"@angular/service-worker": "^16.2.0",
		"@fortawesome/fontawesome-free": "^6.5.2",
		"@wame/blue-oval-theme": "^16.1.0",
		"@wame/ngx-frf-utilities": "^16.1.0",
		"chart.js": "^3.9.1",
		"hammerjs": "~2.0.8",
		"jspdf": "~2.5.1",
		"jspdf-autotable": "~3.8.0",
		"primeflex": "^3.3.1",
		"primeicons": "^6.0.1",
		"primeng": "^16.9.1",
		"quill": "^2.0.1",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.13.0",
		"@ngxs/store": "~3.8.2",
		"@ngxs/form-plugin": "~3.8.2",
		"@wame/ngx-adfs": "^16.1.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^16.2.0",
		"@angular-eslint/builder": "~16.3.0",
		"@angular-eslint/eslint-plugin": "~16.3.0",
		"@angular-eslint/eslint-plugin-template": "~16.3.0",
		"@angular-eslint/schematics": "~16.3.0",
		"@angular-eslint/template-parser": "~16.3.0",
		"@angular/cli": "^16.2.0",
		"@angular/compiler-cli": "^16.2.0",
		"@angular/language-service": "^16.2.0",
		"@compodoc/compodoc": "1.1.16",
		"@playwright/test": "^1.44.0",
		"@types/node": "^18.19.0",
		"@types/jasmine": "~4.3.0",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jasmine-core": "~4.6.0",
		"jasmine-spec-reporter": "~7.0.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"prettier": "^3.2.5",
		"prettier-eslint": "^16.1.2",
		"stylelint": "^15.11.0",
		"stylelint-config-standard-scss": "^11.0.0",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^4.1.0",
		"ts-node": "~10.9.1",
		"typescript": "~5.1.3",
		"sonarqube-scanner": "~3.5.0",
		"@ngxs/devtools-plugin": "~3.8.2"
	},
	"volta": {
		"node": "18.20.2",
		"npm": "10.5.0"
	}
}
